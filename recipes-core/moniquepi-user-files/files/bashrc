alias l='ls -l'
alias rw='elk_system_utils --remount-as-rw'
alias kdui='kill $(pidof delia_ui)'
alias kdgui='kill $(pidof delia_gui)'
alias ksus='kill $(pidof sushi)'
alias psus='ps -e | grep sus'
alias t='top'
alias x='watch -n 0.5 cat /proc/xenomai/sched/stat'
alias startui='systemctl start delia-ui.service'
alias stopui='systemctl stop delia-ui.service'
alias gsus='gdb --ex run --args /usr/bin/sushi_b128 -r -c  ~/delia/config_delia.json'
alias mr='rm /usr/bin/aarch64-elk-linux-lto-dump; rm /usr/bin/bin.tmp'
